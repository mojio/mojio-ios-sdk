use_frameworks!

workspace 'MojioSDK.xcworkspace'

target 'MojioSDK' do
    xcodeproj 'MojioSDK.xcodeproj'

    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'ObjectMapper'
    pod 'RealmSwift'
    pod 'KeychainSwift', '~> 3.0.16'
    pod 'OHHTTPStubs'
    pod 'OHHTTPStubs/Swift'
    pod 'SwiftWebSocket'    
end

target 'MojioSDKTests' do
    pod 'OHHTTPStubs'
    pod 'OHHTTPStubs/Swift'
    pod 'SwiftWebSocket'
end

